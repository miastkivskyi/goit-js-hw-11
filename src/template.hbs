{{#each this}}
  <div class='photo-card'>
    <a class='gallery__link' href={{largeImageURL}}>
      <img src={{webformatURL}} alt={{tags}} loading='lazy' />
    </a>
    <div class='data'>
      <p class='data-item'>
        <b>Likes</b>
        {{likes}}
      </p>
      <p class='data-item'>
        <b>Views</b>
        {{views}}
      </p>
      <p class='data-item'>
        <b>Comments</b>
        {{comments}}
      </p>
      <p class='data-item'>
        <b>Downloads</b>
        {{downloads}}
      </p>
    </div>
  </div>
{{/each}}